<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.HBox?>

<HBox id="menu-view" fx:id="menuView" styleClass="top-header" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ihmTable.controller.MenuController">
   <children>
      <MenuButton id="game-button" fx:id="gameButton" maxHeight="1.7976931348623157E308" mnemonicParsing="false" styleClass="dark-blue-button" text="Partie">
        <items>
            <MenuItem fx:id="informationItem" mnemonicParsing="false" text="Informations" />
            <MenuItem fx:id="quitItem" mnemonicParsing="false" text="Quitter" />
        </items>
      </MenuButton>
      <Label fx:id="title" alignment="CENTER" maxHeight="1.7976931348623157E308" styleClass="h1" text="Unknown" />
   </children>
</HBox>
